<% if object.errors.any? %>
  console.log(<%= pluralize(object.errors.count, 'error') %>);
  <% object.errors.full_messages.each do |error| %>
    console.log(<%= error %>);
  <% end %>
  $('.solution').html('<%= j render "form", object: object %>');
<% else %>
  $('.solution').html('<%= j render "page" %>');
<% end %>
